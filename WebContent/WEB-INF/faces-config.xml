<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
    
    
    <navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>index</from-outcome>
			<to-view-id>index.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>dashboard</from-outcome>
			<to-view-id>dashboard.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>consUsuario</from-outcome>
			<to-view-id>/pages/usuario/consUsuario.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>cadsUsuario</from-outcome>
			<to-view-id>/pages/usuario/cadUsuario.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>consAcesso</from-outcome>
			<to-view-id>/pages/tipoacesso/consAcesso.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>cadAcesso</from-outcome>
			<to-view-id>/pages/tipoacesso/cadAcesso.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>consServicos</from-outcome>
			<to-view-id>/pages/servicos/consServicos.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>cadServicos</from-outcome>
			<to-view-id>/pages/servicos/cadServicos.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>consAlunos</from-outcome>
			<to-view-id>/pages/alunos/consAlunos.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>cadAlunos</from-outcome>
			<to-view-id>/pages/alunos/cadAlunos.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	
	
	<application>
        <action-listener>
            org.primefaces.application.DialogActionListener
        </action-listener>
        <navigation-handler>
            org.primefaces.application.DialogNavigationHandler
        </navigation-handler>
        <view-handler>
            org.primefaces.application.DialogViewHandler
        </view-handler>
    </application>

</faces-config>
